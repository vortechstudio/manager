{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "arcanedev/log-viewer": "^10.0",
        "barryvdh/laravel-dompdf": "^2.0",
        "graham-campbell/github": "^12.4",
        "guzzlehttp/guzzle": "^7.2",
        "jantinnerezo/livewire-alert": "^3.0",
        "larabug/larabug": "^3.0",
        "laravel-notification-channels/facebook-poster": "^6.0",
        "laravel-notification-channels/onesignal": "^2.5",
        "laravel-notification-channels/twitter": "^8.0",
        "laravel/fortify": "^1.20",
        "laravel/framework": "^10.10",
        "laravel/prompts": "*",
        "laravel/sanctum": "^3.3",
        "laravel/socialite": "^5.12",
        "laravel/tinker": "^2.8",
        "livewire/livewire": "^3.4",
        "openai-php/client": "*",
        "pharaonic/laravel-categorizable": "^1.1",
        "pharaonic/laravel-menus": "^1.3",
        "pharaonic/laravel-modulator": "^1.3",
        "pharaonic/laravel-pages": "^1.2",
        "pharaonic/laravel-settings": "^1.2",
        "pharaonic/laravel-sluggable": "^1.0",
        "pharaonic/laravel-taggable": "^1.0",
        "pharaonic/laravel-translatable": "^2.0",
        "pharaonic/livewire-select2": "^1.2",
        "php-flasher/flasher-laravel": "^1.15",
        "php-flasher/flasher-toastr-laravel": "^1.15",
        "salahhusa9/laravel-updater": "^1.0",
        "socialiteproviders/google": "^4.1",
        "spatie/laravel-options": "^1.1",
        "van-ons/laraberg": "^2.0",
        "vstudio/helpers": "^1.0",
        "vstudio/versionbuildaction": "^1.0",
        "wireui/wireui": "^1.18"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
